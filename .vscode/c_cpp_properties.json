{
    "configurations": [
        {
            "name": "Barebone",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "uint8_t=unsigned char",
                "uint16_t=unsigned short",
                "uint32_t=unsigned long",
                "uint64_t=unsigned long long",
                "int8_t=signed char",
                "int16_t=signed short",
                "int32_t=signed long",
                "int64_t=signed long long"
            ],
            "compilerPath": "C:/ITEGCC/bin/arm-none-eabi-gcc.exe",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}